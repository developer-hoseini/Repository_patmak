    <body>
    <div id="jquery-script-menu">
<div class="jquery-script-center">


</div>
</div>
    <h1>jQuery Toaster.js Demo</h1>
        <button onclick="addToast()" >Basic Demo</button>
        <script>
            function addToast(){
                $.Toast("test", "this is a test message.", "success", {
                    has_icon:true,
                    has_close_btn:true,
					stack: true,
                    fullscreen:true,
                    timeout:8000,
                    sticky:false,
                    has_progress:true,
                    rtl:false,
                });
            }
        </script>
